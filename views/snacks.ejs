<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Snacks</title>
    <link href="/css/output.css" rel="stylesheet">
</head>
<body class="bg-gray-100">
    <nav class="bg-white shadow">
        <div class="container mx-auto px-4 py-4">
            <div class="flex justify-between">
                <a href="/" class="text-2xl font-bold">Food Order</a>
                <div>
                    <a href="/auth/logout" class="text-gray-800 hover:text-blue-600">Logout</a>
                </div>
            </div>
        </div>
    </nav>

    <div class="container mx-auto px-4 py-8">
        <h1 class="text-3xl font-bold mb-6">Snacks</h1>
        <div class="grid grid-cols-3 gap-6">
            <div class="bg-white p-6 shadow rounded">
                <img src="url-to-your-image/samosa.jpg" alt="Samosa" class="w-full h-48 object-cover rounded">
                <h2 class="text-xl font-bold mt-2">Samosa</h2>
            </div>
            <div class="bg-white p-6 shadow rounded">
                <img src="url-to-your-image/pakora.jpg" alt="Pakora" class="w-full h-48 object-cover rounded">
                <h2 class="text-xl font-bold mt-2">Pakora</h2>
            </div>
            <div class="bg-white p-6 shadow rounded">
                <img src="url-to-your-image/vada.jpg" alt="Vada" class="w-full h-48 object-cover rounded">
                <h2 class="text-xl font-bold mt-2">Vada</h2>
            </div>
            <div class="bg-white p-6 shadow rounded">
                <img src="url-to-your-image/upma.jpg" alt="Upma" class="w-full h-48 object-cover rounded">
                <h2 class="text-xl font-bold mt-2">Upma</h2>
            </div>
            <!-- Add more snack varieties here -->
        </div>
    </div>

    <script src="/node_modules/flowbite/dist/flowbite.js"></script>
</body>
</html>
